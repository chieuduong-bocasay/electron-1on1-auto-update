{
  "name": "electron-1on1-auto-update",
  "version": "0.0.2",
  "description": "sample test auto update",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron --debug .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder build --publish never",
    "deploy": "electron-builder build --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chieuduong-bocasay/electron-1on1-auto-update.git"
  },
  "author": "Chieu Duong",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.2.4",
    "electron-builder": "^22.3.2"
  },
  "build": {
    "appId": "com.electron.app",
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        "nsis"
      ],
      "verifyUpdateCodeSignature": false
    },
    "mac": {
      "publish": [
        "github"
      ],
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ]
    }
  },
  "dependencies": {
    "electron-updater": "^4.3.5"
  },
  "bugs": {
    "url": "https://github.com/chieuduong-bocasay/electron-1on1-auto-update/issues"
  },
  "homepage": "https://github.com/chieuduong-bocasay/electron-1on1-auto-update#README",
  "keywords": []
}
